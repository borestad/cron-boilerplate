{
  "extends": "@tsconfig/bun/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "rootDir": ".",
    "module": "preserve",
    "moduleResolution": "bundler",
    "checkJs": false,
    "strict": false,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "files": [],
  "include": ["**/*.ts", ".cron/**/*.ts"],
  "exclude": ["deno.json"]
}
